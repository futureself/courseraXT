{
    "manifest_version": 2
    ,"name": "courseraXT"
    ,"description": "Extends functionality of coursera.org"
    ,"version": "1.1"

    ,"permissions": [
        "https://coursera.org/"
    ]
    ,"content_scripts": [{
        "matches": ["*://*.coursera.org/*"]
        ,"js": [
            "jquery.js"
            ,"mutation-summary.js"
            ,"minutes.js"
            ,"step-rewind.js"
        ]
        ,"all_frames": true
    }]
}
